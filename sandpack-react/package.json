    "react-dom": "16.13.1",
    "rimraf": "^2.6.1",
    "storybook-addon-jsx": "^7.3.4",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "4.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17",
    "react-dom": "^16.8.0 || ^17"
  }
